package com.jsp.events {import flash.events.Event;							public class JSEvent extends Event {				public static const GENERIC_EVENT:String = "generic_event";		public static const MOUSE_WHEEL_EVENT:String = "MOUSE_WHEEL_EVENT";				private var _data : *;				/**		 * Data container for event. Used when passing more then an event type to a listener. This can be any value string, number, array or object.		 * @return data : * - Untyped value passed along with event.		 */		public function get data():*{			return _data;		}				public function JSEvent(type:String, data:* = null, bubbles:Boolean = false, cancelable:Boolean = false){	        super(type, bubbles, cancelable);			_data = data;	    }			}}